{
  "mcp_services": [
    {
      "name": "openmemory",
      "type": "stdio",
      "command": "npx",
      "args": ["@openmemory/mcp-server"],
      "capabilities": ["memory_management", "compression", "extraction", "search"],
      "priority": 10,
      "enabled": true,
      "timeout": 30000,
      "retry_count": 3,
      "health_check_interval": 60000,
      "metadata": {
        "description": "OpenMemory MCP服务，用于记忆管理和压缩",
        "version": "1.0.0"
      }
    },
    {
      "name": "filesystem",
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/tmp"],
      "capabilities": ["file_operations", "directory_listing", "file_search"],
      "priority": 8,
      "enabled": true,
      "timeout": 30000,
      "retry_count": 3,
      "health_check_interval": 60000,
      "metadata": {
        "description": "文件系统MCP服务",
        "root_path": "/tmp"
      }
    }
  ],
  "a2a_agents": [
    {
      "name": "coding_agent",
      "type": "http",
      "endpoint": "http://localhost:3001/api/agent",
      "capabilities": ["code_generation", "code_review", "debugging", "refactoring"],
      "specialties": ["javascript", "typescript", "python", "react"],
      "priority": 9,
      "enabled": true,
      "auth": {
        "type": "api_key",
        "credentials": {
          "api_key": "coding-agent-key-123"
        }
      },
      "timeout": 120000,
      "retry_count": 2,
      "health_check_interval": 60000,
      "load_balancing": {
        "strategy": "weighted",
        "weight": 3
      },
      "metadata": {
        "description": "专业编程代理",
        "version": "2.1.0",
        "max_concurrent_tasks": 5
      }
    }
  ],
  "routing_rules": [
    {
      "name": "memory_operations",
      "condition": {
        "keywords": ["记忆", "存储", "回忆", "压缩", "提取"],
        "capabilities_required": ["memory_management"]
      },
      "target": {
        "type": "mcp",
        "name": "openmemory",
        "fallback": ["local"]
      },
      "priority": 10,
      "enabled": true
    },
    {
      "name": "coding_tasks",
      "condition": {
        "keywords": ["代码", "编程", "开发", "调试", "重构"],
        "task_type": "development",
        "capabilities_required": ["code_generation"]
      },
      "target": {
        "type": "agent",
        "name": "coding_agent",
        "fallback": ["local"]
      },
      "priority": 9,
      "enabled": true
    },
    {
      "name": "data_analysis",
      "condition": {
        "keywords": ["分析", "数据", "统计", "报告", "可视化"],
        "capabilities_required": ["data_analysis"]
      },
      "target": {
        "type": "agent",
        "name": "analysis_agent",
        "fallback": ["database"]
      },
      "priority": 8,
      "enabled": true
    },
    {
      "name": "research_tasks",
      "condition": {
        "keywords": ["研究", "调研", "搜索", "信息收集"],
        "capabilities_required": ["research", "information_gathering"]
      },
      "target": {
        "type": "agent",
        "name": "research_agent",
        "fallback": ["web_search"]
      },
      "priority": 7,
      "enabled": true
    },
    {
      "name": "file_operations",
      "condition": {
        "keywords": ["文件", "目录", "读取", "写入", "删除"],
        "capabilities_required": ["file_operations"]
      },
      "target": {
        "type": "mcp",
        "name": "filesystem",
        "fallback": ["local"]
      },
      "priority": 6,
      "enabled": true
    },
    {
      "name": "high_priority_interrupt",
      "condition": {
        "priority_level": "urgent",
        "keywords": ["紧急", "立即", "中断"]
      },
      "target": {
        "type": "local",
        "name": "interrupt_handler",
        "fallback": []
      },
      "priority": 100,
      "enabled": true
    }
  ],
  "memory_config": {
    "provider": "openmemory",
    "openmemory": {
      "mcp_name": "openmemory",
      "compression_threshold": 1000,
      "extraction_interval": 3600000,
      "retention_policy": {
        "max_messages": 10000,
        "max_age_days": 30,
        "importance_threshold": 0.5
      }
    },
    "local": {
      "storage_path": "./.manus/memory",
      "max_file_size": 10485760
    }
  },
  "task_management": {
    "max_concurrent_tasks": 8,
    "task_timeout": 300000,
    "priority_queue_size": 200,
    "interruption_policy": "at_checkpoint",
    "checkpoint_interval": 30000,
    "task_persistence": true,
    "auto_recovery": true
  },
  "decision_engine": {
    "strategy": "hybrid",
    "confidence_threshold": 0.7,
    "fallback_strategy": "priority",
    "learning_enabled": true,
    "metrics_collection": true
  },
  "system": {
    "name": "Manus-MultiAgent-Enhanced",
    "version": "2.1.0",
    "debug_mode": false,
    "log_level": "info"
  }
}
